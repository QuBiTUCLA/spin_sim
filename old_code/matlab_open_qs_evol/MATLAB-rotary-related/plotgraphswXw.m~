close all
figure(56)

load('50cyclespi.mat')
plot(w50/1e9,sig50,'o','MarkerSize',6,'LineWidth',1,'Color','b')
plot(w50/1e9,fit50,'MarkerSize',6,'LineWidth',1,'Color','b')
hold on
plot(w50(res50)/1e9,sig50(res50),'bd','MarkerSize',18)
hold on

load('87cyclespi.mat')
plot(w87(1:1:110)/1e9,sig87(1:1:110),'o',w87(1:1:110)/1e9,fit87(1:1:110),'LineWidth',2,'Color','b','MarkerSize',5)
hold on
plot(w87(res87),sig87(res87),'bd','MarkerSize',18)
hold on

load('164cyclespi.mat')
plot(w164/1e9,sig164,'o',w164/1e9,fit164,'LineWidth',2.5,'Color','b','MarkerSize',4)
hold on
plot(w164(res164)/1e9,sig164(res164),'bd','MarkerSize',18)
hold on

